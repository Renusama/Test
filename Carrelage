public class Carrelage extends Revetement {
   String idCarrelage ;
   double prix; 

    public String getIdCarrelage() {
        return idCarrelage;
    }

    public void setIdCarrelage(String idCarrelage) {
        this.idCarrelage = idCarrelage;
    }

    public double getPrix() {
        return prix;
    }

    public void setPrix(double prix) {
        this.prix = prix;
    }

    public Carrelage(String idCarrelage, double prix) {
        this.idCarrelage = idCarrelage;
        this.prix = prix;
    }

    @Override
    public String toString() {
        return "Carrelage{" + "idCarrelage=" + idCarrelage + ", prix=" + prix + '}';
    }
   
   public double Montant (){
        Montant = prix * surface 
    }
}
